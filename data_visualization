G.add_node(23, label="United States", pos=(-95.712891,37.09024), gdp=sum(A[23]))
for i in range(24):
	for j in range(24):
		if j!=i:
			G.add_edge(i,j,weight=A[i][j])
#plt.figure(figsize=(8,8))
# with nodes colored by degree sized by population
gdp_list=nx.get_node_attributes(G,'gdp')
node_color=[123,35,657,234,456,34563,243,734,8567,98,3456,7546,867,345,234,97,265,5697,235,876,24,2345,7465,635]
node_size=[]
for i in range(24):
	node_size.append(gdp_list[i]/1000.0)
pos=nx.get_node_attributes(G,'pos')
nx.draw(G, pos, node_color=node_color, width=0.3, node_size=node_size)
     #node_size=[G.population[v] for v in H],
